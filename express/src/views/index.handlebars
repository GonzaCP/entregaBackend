<div>
    <h1>soy hello.handlebars, y vos sos:   {{user.name}}, bienvenido!  </h1>


    {{#if isAdmin}}

        {{#each food}}
            <div>
                <p>Nombre: {{this.name}}</p>
                <p>Precio: {{this.price}}</p>
            </div>          
        {{/each}}

        {{else}}
           <p> No tienes permiso para ver la lista de comida!</p>       
       
    {{/if}}

</div>


<script src="/express/src/public/js/index.js"></script>